<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.pay1.community">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">


        <activity
            android:label="Training Material"
            android:name=".training.FeedActivity" >
            <intent-filter android:label="Training">
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs that begin with "http://www.community-training.com” -->
                <data android:scheme="http"
                    android:host="www.community-training.com"
                    />
            </intent-filter>
        </activity>

        <activity
            android:label="Aeps"
            android:name=".aeps.AepsActivity" >
            <intent-filter android:label="AEPS">
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs that begin with "http://www.community-aeps.com” -->
                <data android:scheme="http"
                    android:host="www.community-aeps.com"
                     />
            </intent-filter>
        </activity>

        <activity
            android:label="Blog"
            android:name=".blog.BlogActivity" >
            <intent-filter android:label="Blog">
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs that begin with "http://www.community-blog.com” -->
                <data android:scheme="http"
                    android:host="www.community-blog.com"
                    />
            </intent-filter>

        </activity>

        <activity
            android:label="Company Update"
            android:name=".update.UpdateActivity" >
            <intent-filter android:label="Update">
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs that begin with "http://www.community-update.com” -->
                <data android:scheme="http"
                    android:host="www.community-update.com"
                    />
            </intent-filter>
        </activity>

        <activity
            android:name=".home.HomeActivity" >
            <intent-filter android:label="Home">
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Accepts URIs that begin with "http://www.community-home.com” -->
                <data android:scheme="http"
                    android:host="www.community-home.com"
                    />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>


</manifest>